package com.naver;

public class Test3 {

	private int a;
	private int b;
	
	
	public  Test3() {
		a=0;
		b=0;
	}
	
	public void plus1() {
		System.out.println(a);
		++a;
		System.out.println(a);		
	}
	
	public void plus11() {
		System.out.println(++a);
		
	}
	
	public void plus2() {
		System.out.println(b);
		b++;
		System.out.println(b);
	}
}
