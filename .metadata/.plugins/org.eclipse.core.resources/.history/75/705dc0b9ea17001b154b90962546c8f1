package kr.co.ezen;

public class Duck {
	
	private String a;
	private int b;
	private String c;
	
    public Duck() {
		// TODO Auto-generated constructor stub
	}

	public Duck(String a, int b, String c) {
		
		this.a = a;
		this.b = b;
		this.c = c;
	}

	public String getA() {
		return a;
	}

	public void setA(String a) {
		this.a = a;
	}

	public int getB() {
		return b;
	}

	public void setB(int b) {
		this.b = b;
	}

	public String getC() {
		return c;
	}

	public void setC(String c) {
		this.c = c;
	}
    
    
	}
	


